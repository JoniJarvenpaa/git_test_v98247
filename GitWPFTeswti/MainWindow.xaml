                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button Content="Button" HorizontalAlignment="Left" Margin="491,150,0,0" VerticalAlignment="Top" Click="Button_Click" RenderTransformOrigin="0.5,0.5" Height="72" Width="54">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="1.623" ScaleY="-0.616"/>
                    <SkewTransform AngleY="0" AngleX="45.381"/>
                    <RotateTransform Angle="58.187"/>
